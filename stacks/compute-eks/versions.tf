/*
  versions.tf (stack 04-compute-eks)
  FUNÇÃO: Travar as versões do Terraform e dos Providers.
*/

terraform {
  required_version = ">= 1.5.0"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
    kubernetes = {
      source  = "hashicorp/kubernetes"
      version = "~> 2.23"
    }
  }
}